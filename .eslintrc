{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["import", "jsx-a11y", "react", "react-hooks"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-param-reassign": "off",
    "no-use-before-define": "off",
    "strict": 0
  }
}
